{
	"schemaVersion": 1,
	"id": "ccdrones",
	"version": "${version}",
	"name": "CC:Drones+",
	"description": "CC:Drones+ is a fork of the original CC:Drones! now with more stuff!",
	"authors": [
		"JSJBDEV (Original CCDrones Author)",
        "minecraftbossdj (CCDrones+ Author)"
	],
	"contact": {
		"homepage": "https://github.com/minecraftbossdj/CCDrones",
		"sources": "https://github.com/minecraftbossdj/CCDrones"
	},
	"license": "MIT",
	"icon": "assets/ccdrones/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "ace.actually.ccdrones.CCDrones",
				"adapter": "kotlin"
			}
		],"client": ["ace.actually.ccdrones.ClientInit"]
	},
	"mixins": [
		"ccdrones.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": ">=1.9.24"
	}
}